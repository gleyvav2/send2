{
  "manifest_version": 2,
  "name": "Send2",
  "key":"",
  "description": "Send selected text from the browser to a google doc.",
  "version": "1.0",
  "author": "Gus Leyva",
  "background": {
  "scripts": ["background.js"],
  "js":["s2.js"],
  "persistent":true
  },
  "icons": {
    "16": "send2.png",
    "32": "send2.png",
    "48": "send2.png",
    "128": "send2.png"},
  "permissions":["https://docs.google.com/*","https://apis.google.com/*",
    "https://docs.googleapis.com/*","identity","contextMenus","storage","notifications","tabs"],
  "oauth2": {
    "client_id": "481015228871-q4rf7lfga87fu752pr2tnrgc49tmrv1h.apps.googleusercontent.com",
    "scopes":["https://www.googleapis.com/auth/documents","https://www.googleapis.com/auth/drive.file","https://www.googleapis.com/auth/drive"]
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://apis.google.com/ https://docs.google.com/* ; object-src 'self'",
  
  "browser_action": {
   "default_icon": "send2.png",
   "default_popup": "sendto.html"
  }     
}